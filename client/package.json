{
    "name": "vk-project",
    "version": "0.0.1",
    "description": "Web Interface for VK Project",
    "keywords": [
        "vk",
        "vkui",
        "subscribers"
    ],
    "engines": {
        "node": "v10.16.x",
        "npm": "6.9.x"
    },
    "private": true,
    "repository": {
        "type": "git",
        "url": "https://github.com/mvshmakov/vk-project.git"
    },
    "bugs": {
        "url": "https://github.com/mvshmakov/vk-project/issues"
    },
    "homepage": "https://vk.com/vk-project",
    "main": "App.tsx",
    "proxy": "http://server:8080",
    "scripts": {
        "postinstall": "bash ../scripts/bootstrap.sh",
        "bootstrap": "npm i && npm run start",
        "bootstrap:dev": "npm i && npm run start:dev",
        "bootstrap:prod": "npm i && npm run start:prod",
        "start": "webpack-dev-server --config=configs/webpack/development.js",
        "start:dev": "webpack --watch --config=configs/webpack/development.js",
        "start:prod": "npm run build && webpack-dev-server --config=configs/webpack/production.js",
        "build": "npm run lint && npm run clean:dist && webpack -p --config=configs/webpack/production.js",
        "bundle-analyzer": "BUNDLE_ANALYZER=true && webpack --config=configs/webpack/development.js",
        "clean": "rm -rfd node_modules webpack-bundle-analyzer.html && npm run clean:dist",
        "clean:dist": "rm -rfd dist",
        "lint": "tslint './src/**/*.ts*' --format stylish --force --fix",
        "test": "exit 0 && jest --watch --coverage --config=configs/jest.json"
    },
    "dependencies": {
        "@vkontakte/icons": "^1.8.2",
        "@vkontakte/vk-connect": "^1.5.5",
        "@vkontakte/vkui": "^2.24.1",
        "@vkontakte/vkui-connect-promise": "^0.1.19",
        "connected-react-router": "^6.5.2",
        "history": "^4.9.0",
        "js-cookie": "^2.2.1",
        "normalizr": "^3.4.1",
        "query-string": "^6.8.2",
        "react": "^16.9.0",
        "react-dom": "^16.9.0",
        "react-redux": "^7.1.0",
        "react-router": "^5.0.1",
        "redux": "^4.0.4",
        "redux-actions": "^2.6.5",
        "redux-saga": "^1.0.5"
    },
    "devDependencies": {
        "@babel/cli": "^7.5.5",
        "@babel/core": "^7.5.5",
        "@babel/preset-env": "^7.5.5",
        "@babel/preset-react": "^7.0.0",
        "@types/jest": "^24.0.17",
        "@types/js-cookie": "^2.2.2",
        "@types/react": "^16.9.2",
        "@types/react-dom": "^16.8.5",
        "@types/react-redux": "^7.1.2",
        "@types/react-router": "^5.0.3",
        "@types/redux-actions": "^2.6.1",
        "awesome-typescript-loader": "^5.2.1",
        "babel-loader": "^8.0.6",
        "css-loader": "^3.2.0",
        "file-loader": "^4.2.0",
        "html-webpack-plugin": "^3.2.0",
        "image-webpack-loader": "^5.0.0",
        "jest": "^24.9.0",
        "node-sass": "^4.12.0",
        "sass-loader": "^7.1.0",
        "style-loader": "^1.0.0",
        "tslint": "^5.18.0",
        "types-serviceworker": "0.0.1",
        "typescript": "^3.5.3",
        "uglifyjs-webpack-plugin": "^2.2.0",
        "webpack": "^4.39.1",
        "webpack-bundle-analyzer": "^3.4.1",
        "webpack-cli": "^3.3.6",
        "webpack-config": "^7.5.0",
        "webpack-dev-middleware": "^3.7.0",
        "webpack-dev-server": "^3.7.2",
        "webpack-merge": "^4.2.1"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ]
}
